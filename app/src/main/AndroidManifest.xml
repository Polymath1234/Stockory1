        <activity android:name="com.example.inventorymanagement.VendorCreateAccountActivity" android:exported="false" />
        <activity android:name="com.example.inventorymanagement.MerchantLoginActivity" android:exported="false" />
        <activity android:name="com.example.inventorymanagement.MerchantDashboardActivity" android:exported="false" />
        <activity android:name="com.example.inventorymanagement.MerchantCreateAccountActivity" android:exported="false" />
        <activity android:name="com.example.inventorymanagement.MerchantDetailsActivity" android:exported="false" />
        <activity android:name="com.example.inventorymanagement.ContactActivity" android:exported="false" />
        <activity android:name="com.example.inventorymanagement.AddItemFromMerchantActivity" android:exported="false" />
        <activity android:name="com.example.inventorymanagement.MerchantItemsActivity" android:exported="false" />
        <activity android:name=".SalesHistoryActivity" />
    </application>

</manifest>